  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss --output ./app/styles/tailwind.css --config ./config/tailwind.js --minify",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss --output ./app/styles/tailwind.css --config ./config/tailwind.js --watch",
    "postinstall": "remix setup node",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@nextui-org/react": "^2.2.9",
    "@remix-run/node": "^2.6.0",
    "@remix-run/react": "^1.19.3",
    "@remix-run/serve": "^1.19.3",
    "@remix-run/server-runtime": "^1.0.0",
    "@remix-run/vercel": "^1.19.3",
    "@types/pure-render-decorator": "^0.2.31",
    "@types/react": "^18.2.55",
    "@types/react-addons-css-transition-group": "^15.0.10",
    "@types/react-dom": "^18.2.19",
    "@vercel/analytics": "^0.1.11",
    "framer-motion": "^11.0.3",
    "isbot": "^4.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "remix": "^1.19.3",
    "rss-to-json": "^2.1.1",
    "tailwindcss": "^2.2.19",
    "vue-template-compiler": "^2.7.16"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.19.3",
    "concurrently": "^6.5.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14"
